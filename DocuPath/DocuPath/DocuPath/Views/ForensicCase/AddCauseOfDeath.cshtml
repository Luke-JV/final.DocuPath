@section Scripts{
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/toggle")
    @Scripts.Render("~/bundles/moment")
    @Scripts.Render("~/bundles/bootgrid")
    @Scripts.Render("~/bundles/materialdatetime")

    <script name="CODToggles" type="text/javascript">
            $('textarea').autogrow();
            $('textarea').addClass("form-control");
            $('.typeahead').addClass("fullwidth");
            $(document).ready(function () {
                $("#primarytoggle").checked = true
                $("#2ndCOD").hide()
                $("#3rdCOD").hide()
                $("#4thCOD").hide()
                $('[data-toggle="tooltip"]').tooltip();
            });

            $(function toggleShowCODPanel() {
                $('input[type=checkbox]').change(function () {
                    if ($('#secondarytoggle').prop("checked") == true) { $('#2ndCOD').show('slow'), $('#tertiarytoggle').bootstrapToggle('enable') } else { $('#2ndCOD').hide('slow'), $('#3rdCOD').hide('slow'), $('#4thCOD').hide('slow'), $('#tertiarytoggle').bootstrapToggle('disable') };
                    if ($('#tertiarytoggle').prop("checked") == true) { $('#3rdCOD').show('slow'), $('#quaternarytoggle').bootstrapToggle('enable'), $('#secondarytoggle').bootstrapToggle('disable') } else { $('#3rdCOD').hide('slow'), $('#4thCOD').hide('slow'), $('#quaternarytoggle').bootstrapToggle('disable'), $('#secondarytoggle').bootstrapToggle('enable') };
                    if ($('#quaternarytoggle').prop("checked") == true) { $('#4thCOD').show('slow'), $('#secondarytoggle').bootstrapToggle('disable'), $('#tertiarytoggle').bootstrapToggle('disable') } else { $('#4thCOD').hide('slow') };
                });
            })
    </script>
    <script>
            $("#btnNext").click(function () {
                document.getElementById("btnPOST").click();
            })
    </script>
}