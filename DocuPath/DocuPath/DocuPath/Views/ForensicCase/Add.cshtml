                if ($('#toggleMedTreat_Other').prop("checked") == true) { $('#frmMedTreatOther').show('slow') } else { $('#frmMedTreatOther').hide('slow') };
                if ($('#toggleSOI_Other').prop("checked") == true) { $('#frmSOIOther').show('slow') } else { $('#frmSOIOther').hide('slow') };
