<script>
        $('#btnSave').click(function () {
            var btn = document.getElementById('btnPOST');
            btn.click();
        });
</script>
<script name="warningscript" type="text/javascript">
        $(document).ready(function () {
            $('#btnBack').click(function (event) { event.preventDefault(); $('#divWarning').removeClass("hidden"); });
            $('#btnWarningYes').click(function (target) { window.location.href = "@Url.Action("All", "ForensicCase")"; });
            $('#btnWarningNo').click(function () { $('#divWarning').addClass("hidden"); });
        });
</script>
    @* BACK TO TOP: *@
    <script>
        jQuery(document).ready(function () {
            $('.bottom-banner').css({ "display": "none" });
            var offset = 250;
            var duration = 300;
            jQuery(window).scroll(function () {
                if (jQuery(this).scrollTop() > offset) {
                    jQuery('.bottom-banner').fadeIn(duration);
                } else {
                    jQuery('.bottom-banner').fadeOut(duration);
                }
            });

            jQuery('.bottom-banner').click(function (event) {
                event.preventDefault();
                jQuery('html, body').animate({ scrollTop: 0 }, duration);
                return false;
            })
        });
    </script>

    <script name="CODToggles" type="text/javascript">
        $('textarea').autogrow();
        $('textarea').addClass("form-control");
        $('.typeahead').addClass("fullwidth");
