        $(".titleList").append('<tr class="titleRow"><td class="table-large-identifier text-center">' + (rowCount+1) + '</td><td><input class="form-control frmctrl-small text-box single-line" id="titleValue" name="titles[' + rowCount +'].TitleValue" onclick="this.select()" onfocus="this.select()" style="width: 100%" type="text" value=""></td><td><a id="remTRow" class="mdl2icon colour-dpblack" href="javascript:void(0)" style="padding-top: 2.5px"><i class="mdl2-delete" data-toggle="tooltip" title="" data-placement="right" style="font-style: normal" data-original-title="DELETE"></i></a></td></tr>');

    });

    $(".titleList").on('click', '#remTRow', function () {

        $(this).closest('.titleRow').remove();

        $('#titleID').each(function (index) {
            $(this).attr('name', 'titles[' + index + '].TitleID');
        });
        $('#titleValue').each(function (index) {
            $(this).attr('name', 'titles[' + index + '].TitleValue');
        });


    })
});

